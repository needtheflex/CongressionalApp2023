				Background="rgba(255, 221, 239, 0.8)"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				CornerRadius="50"
				BorderThickness="2"
				BorderBrush="Snow"
				FontWeight="DemiBold"
				Foreground="Snow"
				FontSize="15"
				Click="calculateSimpleInterest">
				calculate
			</Button>
			<Button
				Margin="100,0,0,0"
				Width="150"
				Height="50"
				Background="rgba(255, 221, 239, 0.8)"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				CornerRadius="50"
				BorderThickness="2"
				BorderBrush="Snow"
				FontWeight="DemiBold"
				Foreground="Snow"
				FontSize="15"
				Click="simpleInterestSeeGraph">
				see graph
			</Button>

			<TextBlock
				Margin="95,10,10,10"
				FontWeight="DemiBold"
				FontSize="15"
				Foreground="Snow"
				Name="simpleInterestValue">
			</TextBlock>
		</StackPanel>

		<StackPanel
		Name="compoundInterestCalculator"
		IsVisible="True"
		HorizontalAlignment="Center">
			<TextBlock
				Classes="directions"
				FontWeight="DemiBold"
				FontSize="30"
				TextDecorations="Underline"
				PointerReleased="returnToFinancialCalculator">
				compound interest calculator
			</TextBlock>

			<StackPanel
				HorizontalAlignment="Center"
				Margin="0,-10,0,20">
				<Grid
					ShowGridLines="False"
					Margin="5"
					ColumnDefinitions="210, 200"
					RowDefinitions="50, 50, 50, 50">
					<TextBlock
						Grid.Row="0"
						Grid.Column="0"
						Classes="pricePromptTextBlock">
						principle price
					</TextBlock>
					<TextBox
						Grid.Row="0"
						Grid.Column="1"
						Name="compoundInterestPrinciplePriceUserInput"
						Classes="priceAnswerTextBox">
					</TextBox>
					<TextBlock
					Grid.Row="1"
					Grid.Column="0"
					Classes="pricePromptTextBlock">
						interest rate
					</TextBlock>
					<TextBox
					Grid.Row="1"
					Grid.Column="1"
					Name="compoundInterestRateUserInput"
					Classes="percentAnswerTextBox">
					</TextBox>
					<TextBlock
					Grid.Row="2"
					Grid.Column="0"
					Classes="pricePromptTextBlock">
						years into future
					</TextBlock>
					<TextBox
					Grid.Row="2"
					Grid.Column="1"
					Name="compoundInterestYearsIntoFutureUserInput"
					Classes="yearsAnswerTextBox">
					</TextBox>
					<TextBlock
						Grid.Row="3"
						Grid.Column="0"
						Classes="pricePromptTextBlock">
						future value ($)
					</TextBlock>
					<TextBox
						Grid.Row="3"
						Grid.Column="1"
						Classes="yearsAnswerTextBox"
						Name="compoundInterestFutureValueUserInput"></TextBox>
				</Grid>
			</StackPanel>

			<Button
				Margin="100,0,20,5"
				Width="150"
				Height="50"
				Background="rgba(255, 221, 239, 0.8)"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				CornerRadius="50"
				BorderThickness="2"
				BorderBrush="Snow"
				FontWeight="DemiBold"
				Foreground="Snow"
				FontSize="15"
				Click="calculateCompoundInterest">
				calculate
			</Button>

			<Button
				Margin="100,0,0,0"
				Width="150"
				Height="50"
				Background="rgba(255, 221, 239, 0.8)"
				HorizontalContentAlignment="Center"
				VerticalContentAlignment="Center"
				CornerRadius="50"
				BorderThickness="2"
				BorderBrush="Snow"
				FontWeight="DemiBold"
				Foreground="Snow"
				FontSize="15"
				Click="compoundInterestSeeGraph">
				see graph
			</Button>
			
			<TextBlock
				Margin="95,10,10,10"
				FontWeight="DemiBold"
				FontSize="15"
				Foreground="Snow"
				Name="compoundInterestValue">
			</TextBlock>
		</StackPanel>
	</Panel>
</Window>
